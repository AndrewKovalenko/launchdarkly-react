{
  "name": "react-feature-manager",
  "version": "0.0.3",
  "description": "React toolset to start using feature-flags in your react application",
  "main": "build/umd/react-feature-manager.min.js",
  "module": "build/module/index.js",
  "author": "Andrii Kovalenko",
  "license": "MIT",
  "scripts": {
    "uglify": "uglifyjs build/umd/react-feature-manager.js --output build/umd/react-feature-manager.min.js",
    "build:dev": "export BABEL_ENV=rollup; rollup -c rollup.config.lib.js --watch",
    "build": "export BABEL_ENV=rollup; rollup -c rollup.config.lib.js && npm run uglify",
    "lint": "eslint src",
    "start": "concurrently \"npm run build:dev\" \"export BABEL_ENV=rollup; rollup -c rollup.config.example.js --watch\"",
    "test": "jest --coverage"
  },
  "peerDependencies": {
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "prop-types": "15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.6.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "8.4.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-watch": "^4.3.1",
    "vertibar": "^0.0.32"
  },
  "dependencies": {
    "uglify-js": "^3.4.9"
  }
